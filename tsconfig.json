{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "rootDir": "./src",
    "outDir": "./dist",
    "allowJs": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "paths": {},
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": ["./dist"],
  "include": ["src/**/*"]
}
